# API
GET     /api                        controllers.api.RootController.index
GET     /ws/import                  controllers.api.ImportController.socket

GET     /							controllers.web.IndexController.index
GET     /import						controllers.web.ImportController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.web.Assets.versioned(path="/public", file: Asset)
